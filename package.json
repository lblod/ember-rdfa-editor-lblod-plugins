    "serve": "ember serve",
    "test": "concurrently --group pnpm:lint pnpm:test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "release-it",
    "prepack": "./scripts/generate-type-declarations.sh",
