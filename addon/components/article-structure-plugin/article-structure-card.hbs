{{#unless @controller.inEmbeddedView}}
<div>
  {{#each this.structureTypes as |structureType index|}}
  <AuList::Item>
    <AuButton @icon="add" @iconAlignment="left" @skin="link" @disabled={{(not (get this.canInsertStructures.value index))}} {{on 'click'
      (fn this.insertStructure structureType)}}>
      {{t structureType.translations.insert}}
    </AuButton>
  </AuList::Item>
  {{/each}}
</div>
{{/unless}}