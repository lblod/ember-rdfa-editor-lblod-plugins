<li class="au-c-list__item">
  <AuButton
    @icon='add'
    @iconAlignment='left'
    @skin='link'
    {{on 'click' this.openModal}}
  >
    {{t 'snippet-plugin.insert.title'}}
  </AuButton>
</li>

<SnippetPlugin::SearchModal
  @open={{this.showModal}}
  @closeModal={{this.closeModal}}
  @config={{this.config}}
  @onInsert={{this.onInsert}}
  @assignedSnippetListsIds={{@assignedSnippetListsIds}}
/>