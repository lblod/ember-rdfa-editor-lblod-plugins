{{#if @snippets.length}}
  <AuList @direction='vertical'>
    {{#each @snippets as |snippet|}}
      <AuList::Item>
        <SnippetPlugin::Snippets::SnippetPreview
          @snippet={{snippet}}
          @config={{this.config}}
          @onInsert={{@onInsert}}
        />
      </AuList::Item>
    {{/each}}
  </AuList>
{{/if}}