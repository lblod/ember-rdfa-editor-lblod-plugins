{{#each this.structures as |structure|}}
  <AuList::Item>
    <AuButton
      @icon="add"
      @iconAlignment="left"
      @skin="link"
      @disabled={{false}}
      {{on 'click' (fn this.insertStructure structure.title)}}>
      {{t structure.translation}}
    </AuButton>
  </AuList::Item>
{{/each}}
